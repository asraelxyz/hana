x = "a"
func outer()
    x = "b"
    func inner()
        x = "c"
        print("inner ", x, "\n")
    end
    inner()
    print("outer ", x, "\n")
end

outer()
print("global ", x, "\n")

func adder(n)
    func inner(x)
        return n + x
    end
    return inner
end

x = adder(5)
print(x(10),"\n")

